function multiply("a", "b")
│ if  lhs < rhs
│ │ (lhs) Variable("a")
│ │ (rhs) Variable("b")
│ then
│ │ return multiply(...)
│ │ │ │ Variable("b")
│ │ │ │ Variable("a")
│ else
│ │ iterations = Variable("b")
│ │ while  lhs > rhs
│ │ │ (lhs) Variable("iterations")
│ │ │ (rhs) Integer("1")
│ │ then
│ │ │ a =  lhs + rhs
│ │ │ │ (lhs) Variable("a")
│ │ │ │ (rhs) Variable("a")
│ │ │ iterations =  lhs - rhs
│ │ │ │ (lhs) Variable("iterations")
│ │ │ │ (rhs) Integer("1")
│ │ return Variable("a")
